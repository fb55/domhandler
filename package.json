{
  "name": "domhandler",
  "version": "2.5.0",
  "description": "handler for htmlparser2 that turns pages into a dom",
  "main": "index.js",
  "types": "./dist/dom-handler.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "lint": "jshint index.js test/ && tslint --project tsconfig.json",
    "test": "tsc && mocha -R list && mocha -R list --globals typed && jshint index.js test/ && tslint --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fb55/DomHandler.git"
  },
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "dependencies": {
    "domelementtype": "1"
  },
  "devDependencies": {
    "htmlparser2": "^3.9.0",
    "mocha": "^3.0.2",
    "jshint": "^2.9.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "author": "Felix Boehm <me@feedic.com>",
  "license": "BSD-2-Clause",
  "jshintConfig": {
    "quotmark": "double",
    "trailing": true,
    "unused": true,
    "undef": true,
    "node": true,
    "proto": true,
    "globals": {
      "it": true
    }
  }
}
